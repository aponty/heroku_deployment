<script src='./engines/hand-1.3.7.js'></script>
<script src='./engines/cannon.js'></script>
<script src='./engines/babylon.js'></script>
<script src='./engines/materials/waterMaterial.js'></script>
<canvas id='canvas'></canvas>
<p id="gunsight"/>x</p>
<div id="lifeLabel">LIFE : 150</div>
<div id="ammoLabel">AMMO : 30</div>
<div id="scoreLabel">SCORE : 0</div>

<div class="modalBackground">
<div class="modal">
    <%= form_for(:levels, url: '/levels', :method => "post") do |f| %>
    <div class="input-group">
        <%= f.label :Name %>
        <%= f.text_field :name %>
    </div>
    <%= f.hidden_field :score, :value => "0" %>
    <%= f.hidden_field :level, :value => "3" %>
    <%= f.submit :value => 'submit' %>
    <% end %>
</div>
</div>
<script src="./fps/main.js"></script>
